{
	"name": "obsidian-sample-plugin",
  "version": "0.0.1",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "build:mode": "vite build",
    "build": "pnpm run prod",
    "dev": "pnpm run build:mode --mode development",
    "prod": "pnpm run typecheck && pnpm run lint:fix && pnpm run format:fix && pnpm run build:mode --mode production",
    "release": "pnpm run prod && pnpm run version && git commit -m 'chore: bump version'",
    "release:beta": "TBD",
    "version": "node version-bump.js && git add manifest.json versions.json package.json",
    "typecheck": "tsc --noEmit -skipLibCheck",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "GitMurf",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@html-eslint/eslint-plugin": "^0.20.0",
    "@html-eslint/parser": "^0.20.0",
    "@types/node": "^20.9.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@typescript-eslint/utils": "^6.12.0",
    "@vitest/coverage-v8": "1.0.0-beta.5",
    "@vitest/ui": "1.0.0-beta.5",
    "builtin-modules": "3.3.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-css": "^0.8.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-mdx": "^2.2.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-yml": "^1.10.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.1.0",
    "obsidian": "latest",
    "prettier": "^3.1.0",
    "remark-preset-lint-consistent": "^5.1.2",
    "remark-preset-lint-markdown-style-guide": "^5.1.3",
    "remark-preset-lint-recommended": "^6.1.3",
    "remark-preset-prettier": "^2.0.1",
    "ts-node": "^10.9.1",
    "tslib": "2.6.2",
    "typedoc": "^0.25.3",
    "typescript": "~5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "1.0.0-beta.5"
  }
}
